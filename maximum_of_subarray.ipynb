      "source": [
        "def maxofsubarray(arr,k):\n",
        "  n=len(arr)\n",
        "\n",
        "  res=[]\n",
        "  for i in range(n-k+1):\n",
        "    max_ele=max(arr[i:i+k])\n",
        "    res.append(max_ele)\n",
        "  return res\n",
        "\n",
        "arr=[1,2,3,1,4,5,2,3,6]\n",
        "k=3\n",
        "print(maxofsubarray(arr,k))"
      ]
